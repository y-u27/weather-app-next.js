generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model City {
  id Int @id @default(autoincrement())
  name String @unique
  queryName String
  weathers Weather[]
}

model Weather {
  id Int @id @default(autoincrement())
  weather String
  temperature Float
  observed_at DateTime
  cityId Int
  citys City @relation(fields: [cityId], references: [id])
}

